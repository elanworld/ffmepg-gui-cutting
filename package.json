{
  "name": "ffmepg-gui-cutting",
  "version": "1.0.0",
  "description": "cross platform app",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.ffmpeg.gui",
    "productName": "ffmepg gui",
    "directories": {
      "output": "dist"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "portable"
    },
    "linux": {
      "target": [
        "AppImage"
      ]
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@electron-forge/cli": "^7.3.0",
    "electron": "^29.1.1",
    "electron-builder": "^24.13.3"
  },
  "dependencies": {
    "node-fetch": "^2.6.1",
    "unzipper": "^0.10.14"
  }
}
